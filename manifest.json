{
  "name": "TS Chrome Extension Example",
  "description": "Build a Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "dist/background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
